available_foods = [
    "pizza",
    "sushi",
    "carrot cake",
    "cannoli",
    "ice cream",
    "hot dog",
    "sandwich",
]

available_crews = 5


each_crew_food =  len(available_foods) // available_crews

remaining_food_count = len(available_foods) % available_crews

print(f"Each crew get {each_crew_food} food and remaining food count = {remaining_food_count}")